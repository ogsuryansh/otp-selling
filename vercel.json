{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/users",
      "dest": "/api/index.js"
    },
    {
      "src": "/transactions",
      "dest": "/api/index.js"
    },
    {
      "src": "/servers",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin",
      "dest": "/admin_fixed.html"
    },
    {
      "src": "/",
      "dest": "/admin_fixed.html"
    },
    {
      "src": "/debug-dashboard.html",
      "dest": "/debug-dashboard.html"
    },
    {
      "src": "/admin_panel.html",
      "dest": "/admin_panel.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
