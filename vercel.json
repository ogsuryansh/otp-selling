{
  "version": 2,
  "name": "otp-bot-admin-panel",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/admin_fixed.html"
    },
    {
      "src": "/admin",
      "dest": "/admin_fixed.html"
    },
    {
      "src": "/admin-panel",
      "dest": "/admin_fixed.html"
    },
    {
      "src": "/login",
      "dest": "/admin_fixed.html"
    },
    {
      "src": "/dashboard",
      "dest": "/admin_fixed.html"
    },
    {
      "src": "/users",
      "dest": "/admin_fixed.html"
    },
    {
      "src": "/transactions",
      "dest": "/admin_fixed.html"
    },
    {
      "src": "/servers",
      "dest": "/admin_fixed.html"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
