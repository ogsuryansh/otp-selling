{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/users",
      "dest": "/public/users.html"
    },
    {
      "src": "/transactions",
      "dest": "/public/transactions.html"
    },
    {
      "src": "/statistics",
      "dest": "/public/statistics.html"
    },
    {
      "src": "/admin",
      "dest": "/admin_panel.html"
    },
    {
      "src": "/debug-dashboard.html",
      "dest": "/debug-dashboard.html"
    },
    {
      "src": "/admin_panel.html",
      "dest": "/admin_panel.html"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    }
  ]
}
